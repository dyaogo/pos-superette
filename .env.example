# Example environment variables for the application
# Copy this file to `.env` and fill in the values.

# Firebase configuration
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_MEASUREMENT_ID=

# Application settings
GENERATE_SOURCEMAP=false
REACT_APP_VERSION=
REACT_APP_ENVIRONMENT=
REACT_APP_BUILD_DATE=

# Vercel Blob Storage (pour les images)
# Obtenir sur: https://vercel.com/dashboard/stores
BLOB_READ_WRITE_TOKEN=

# Sentry Error Monitoring
# CrÃ©er un projet sur: https://sentry.io/
# DSN disponible dans: Project Settings > Client Keys (DSN)
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=
# Token d'authentification pour upload des source maps (optionnel)
SENTRY_AUTH_TOKEN=
